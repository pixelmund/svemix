{
	"name": "svemix",
	"version": "0.9.2",
	"description": "The Full-Stack addition to SvelteKit. Write your server code inside svelte files, handle sessions, forms and SEO easily.",
	"repository": {
		"type": "git",
		"url": "https://github.com/svemix/svemix.git"
	},
	"homepage": "https://svemix.com",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env TEST=true vite dev",
		"build": "cross-env PROD=true node pre-build-fix.js && svelte-package",
		"build:kit": "cross-env TEST=true vite build",
		"preview": "cross-env TEST=true vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --ignore \"dist,build,sites\"",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "cross-env TEST=true playwright test"
	},
	"dependencies": {
		"prettier": "^2.4.1",
		"zencrypt": "^0.0.7"
	},
	"devDependencies": {
		"@playwright/test": "^1.17.1",
		"@sveltejs/kit": "^1.0.0-next.428",
		"@sveltejs/package": "^1.0.0-next.1",
		"@types/node": "^17.0.0",
		"@types/prettier": "^2.4.3",
		"cross-env": "^7.0.3",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.8",
		"svelte2tsx": "^0.4.11",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"vite": "^3.0.0"
	},
	"type": "module",
	"keywords": [
		"svemix",
		"svelte server-only scripts",
		"server only load",
		"sveltekit getServerSideProps",
		"full-stack svelte",
		"svelte seo",
		"sveltekit seo",
		"sveltekit session",
		"svelte remix",
		"svelte",
		"forms"
	]
}
