{
	"name": "svemix",
	"version": "0.6.2",
	"description": "The Full-Stack addition to SvelteKit. Write your server code inside svelte files, handle sessions, forms and SEO easily.",
	"repository": {
		"type": "git",
		"url": "https://github.com/svemix/svemix.git"
	},
	"homepage": "https://svemix.com",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env TEST=true svelte-kit dev --port=3001",
		"build": "svelte-kit package && npm run format",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "cross-env TEST=true playwright test"
	},
	"devDependencies": {
		"@playwright/test": "^1.17.1",
		"@sveltejs/kit": "next",
		"@types/node": "^17.0.0",
		"@types/prettier": "^2.4.3",
		"cross-env": "^7.0.3",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.8",
		"svelte2tsx": "^0.4.11",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"vite": "^2.7.10"
	},
	"type": "module",
	"keywords": [
		"svemix",
		"svelte server-only scripts",
		"server only load",
		"sveltekit getServerSideProps",
		"full-stack svelte",
		"svelte seo",
		"sveltekit seo",
		"sveltekit session",
		"svelte remix",
		"svelte",
		"forms"
	]
}
